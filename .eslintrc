{
  "env": {
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends":[ "eslint:recommended", "plugin:jest/recommended" ],
  "plugins": ["jest"],
  "rules": {
    "comma-dangle": ["error", "never"],
    "semi": ["error", "never"]
  }
}
